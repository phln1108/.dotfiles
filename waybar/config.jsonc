{
    "layer": "top",
      "modules-left": ["custom/arch", "hyprland/workspaces"],
      "modules-center": ["clock"],
      "modules-right": ["backlight", "pulseaudio", "bluetooth", "network", "battery"],
      "custom/arch": {
        "format": " (0_0) ",
        "tooltip": false,
        "on-click": "sh $HOME/.config/rofi/bin/powermenu"
      },
      "hyprland/workspaces": {
        "format": "{icon}{name}{icon}",
        // "format": "{icon}",
        "tooltip": false,
        "all-outputs": true,
        "format-icons": {
          "active": "|",
          // "active": "ï†’",
          "default": ""
          // "default": "ï„Œ"
        }
      },
      "clock": {
        "format": "<span color='#b4befe'>ï€— </span>{:%H:%M}",
      },
      "backlight": {
        "device": "intel_backlight",
        "format": "<span color='#b4befe'>{icon}</span> {percent}%",
        "format-icons": ["î", "î“", "î‘", "î", "î", "î", "îŠ", "îˆ", "î›"]
      },
      "pulseaudio": {
        "format": "<span color='#b4befe'>{icon}</span> {volume}%",
        "format-muted": "ï€¦",
        "tooltip": false,
        "format-icons": {
          "headphone": "ï€¥",
          "default": ["ï€¦", "ï€§", "ó°•¾", "ó°•¾", "ó°•¾", "ï€¨", "ï€¨", "ï€¨"]
        },
        "scroll-step": 1,
      },
      "bluetooth": {
        "format": "<span color='#b4befe'>ïŠ”</span> {status}",
        "format-disabled": "", // an empty format will hide the module
        "format-connected": "<span color='#b4befe'>ïŠ”</span> {num_connections}",
        "tooltip-format": "{device_enumerate}",
        "tooltip-format-enumerate-connected": "{device_alias}   {device_address}"
      },
      "network": {
        // "interface": "wlp2*", // (Optional) To force the use of this interface
        "format-wifi": "{essid} ({signalStrength}%) ï‡«",
        "format-ethernet": "{ipaddr}/{cidr} ï–",
        "tooltip-format": "{ifname} via {gwaddr} ï–",
        "format-linked": "{ifname} (No IP) ï–",
        "format-disconnected": "Disconnected âš ",
        "format-alt": "{ifname}: {ipaddr}/{cidr}"
    },  
    "battery": {
        "format": "<span color='#b4befe'>{icon}</span> {capacity}%",
        "format-icons": ["ï•¹", "ï•º", "ï•»", "ï•¼", "ï•½", "ï•¾", "ï•¿", "ï–€", "ï–", "ï•¸"],
        "format-charging": "<span color='#b4befe'>ï–ƒ</span> {capacity}%",
        "tooltip": false,
      },
  }
